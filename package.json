{
  "author": "QWAN - Quality Without a Name",
  "name": "vendingrefact_js",
  "description": "Exercise for code smells & refactoring with JavaScript and Jasmine",
  "version": "1.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/qwaneu/vendingrefact_js"
  },
  "watch": {
    "test": {
      "patterns": [
        "lib",
        "spec"
      ],
      "extensions": "js",
      "quiet": true
    }
  },
  "scripts": {
    "test": "jasmine spec/**/*.js",
    "watch_test": "npm-watch test",
    "jshint": "jshint --config jshint.json spec/*.js lib/*.js",
    "bundle": "webpack ./lib/app.js ./dist/bundle.js --mode development"
  },
  "engines": {
    "node": "~14.15.4"
  },
  "dependencies": {
    "jquery": "^3.6.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "canvas": "^2.8.0",
    "jasmine": "~3.7.0",
    "jsdom": "^16.6.0",
    "jshint": "^2.12.0",
    "npm-watch": "^0.9.0",
    "webpack": "^5.38.0",
    "webpack-cli": "^4.7.0"
  }
}
